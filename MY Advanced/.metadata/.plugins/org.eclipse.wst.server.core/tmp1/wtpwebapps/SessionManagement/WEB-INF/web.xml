    <servlet-name>ProfileServlet</servlet-name>
    <servlet-class>EventListener.ProfileServlet</servlet-class>
  </servlet>
  <servlet-mapping> 
    <servlet-name>ProfileServlet</servlet-name>
    <url-pattern>/onEvent</url-pattern>
  </servlet-mapping> -->
  <listener>
    <listener-class>EventListener.LoginListener</listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>link.html</welcome-file>
    <welcome-file>user.html</welcome-file>
    <welcome-file>event.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>MyServlet</servlet-name>
  <servlet-class>Filters.MyServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>MyServlet</servlet-name>
  <url-pattern>/MyServlet</url-pattern>
  </servlet-mapping>
  
  <filter>
  <filter-name>MyFilter</filter-name>
  <filter-class>Filters.MyFilter</filter-class>
  <init-param>
  <param-name>name</param-name>
  <param-value>mahe</param-value>
  </init-param>
  </filter>  
  <filter-mapping>
  <filter-name>MyFilter</filter-name>
  <url-pattern>/MyServlet</url-pattern>
  </filter-mapping>
</web-app>